  .recaptcha_image_cell { width: auto !important; padding: 0px !important; }
  #recaptcha_table tr > td:last-child { display: none; }
  #recaptcha_table tr[height="73"] { height: auto !important; }
  #recaptcha_table tr > td { padding: 0px !important; }
  #recaptcha_image { width: 280px !important; }
  .recaptchatable .recaptcha_image_cell center { }
  #recaptcha_response_field { margin-left: 3px; margin-top: -1px; width: 272px !important; font-size: 10pt !important; }
  #recaptcha_image > img { width: 280px !important; }
  #postForm:not(.hideMobile) { overflow: hidden; margin-top: 20px; display: table; }
  form[name="post"] { margin: auto; max-width: 100%; }
  input[type="text"], input[type="password"], textarea { appearance: none; border-radius: 0px; }
  input:focus, textarea:focus { border: 1px solid rgb(238, 170, 136) !important; }
  a, .useremail:not(:hover) .name:not(.capcode), .useremail:not(:hover) .postertrip:not(.capcode) { color: rgb(0, 0, 238) !important; }
  a.replylink, div#absbot a { text-decoration: underline !important; }
  a.replylink:not(:hover), div#absbot a:not(:hover) { color: rgb(0, 0, 238) !important; }
  a:hover, .useremail .name:hover, .useremail .postertrip:hover, a.quoteLink:hover, a.quotelink:hover, a.deadlink:hover, .useremail :hover, .useremail:hover *, .posteruid .hand:hover { color: red !important; }
  .quoteLink, .quotelink, .deadlink, .pageJump a { color: navy !important; }
  .button a, a.button:hover, .button a:visited { color: rgb(136, 0, 0) !important; }
  a.redButton:hover, a.redButton:focus { color: rgb(136, 0, 0) !important; }
  table#recaptcha_table > tbody > tr:first-child > td:nth-child(2) { display: none; }
  div.reply { background-color: rgb(240, 224, 214); border: 1px solid rgb(217, 191, 183) !important; }
  .reply:target, .reply:focus, .reply.highlight { background: rgb(240, 192, 176) !important; border: 1px solid rgb(217, 191, 183) !important; }
